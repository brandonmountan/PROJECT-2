                        <input type="text" class="form-control" name="query" placeholder="Search for items..." required>
                        <div class="input-group-append">
                            <button type="submit" class="btn btn-primary">Search</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>

